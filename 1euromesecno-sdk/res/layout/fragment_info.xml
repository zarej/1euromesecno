<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="rs.hakaton.euromesecno.sdk.InfoActivity$PlaceholderFragment" >
	
    
    <include layout="@layout/main_list_item"/>
    
    <include layout="@layout/line"/>
    
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Potrebno"/>
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_amount"/>
        </LinearLayout>
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
             <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Boluje od"/>
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_problem"/>
        </LinearLayout>
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            
             <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="SMS Broj:"/>
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_sms_number"/>
             <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="text:"/>
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_sms_text"/>
            
        </LinearLayout>
    </LinearLayout> 
    
    <include layout="@layout/line"/>
    
    <TextView 
        android:id="@+id/info_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        />
    
    <include layout="@layout/line"/>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
        	
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="DINARSKI RAUČN"/>
        
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
        	
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_domestic_account"/>
        
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
        
        	<TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_domestic_bank"/>
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
        	
            
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA IME"/>
            
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_domestic_account_holder"/>
        
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
        	
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="DEVIZNI RAČUN"/>
            
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
        	
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_foreign_bank_account"/>
        
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
            
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="SWIFT:"/>
        	
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_foreign_bank_swift"/>
        
    	</LinearLayout>
    	
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">
            
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="IBAN:"/>
        	
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/info_foreign_bank_iban"/>
        
    	</LinearLayout>
    </LinearLayout>
	
	<include layout="@layout/line"/>
	
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical">
	    
	    <LinearLayout 
	         android:layout_width="match_parent"
	    	 android:layout_height="wrap_content"
	    	 android:orientation="horizontal">
	        
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="KONTAKT"/>
	    </LinearLayout>
	    
	    <LinearLayout 
	         android:layout_width="match_parent"
	    	 android:layout_height="wrap_content"
	    	 android:orientation="horizontal">
	        
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="Email:"/>
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:id="@+id/info_email"/>
	    </LinearLayout>
	    
	    <LinearLayout 
	         android:layout_width="match_parent"
	    	 android:layout_height="wrap_content"
	    	 android:orientation="horizontal">
	        
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="Telefon: "/>
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:id="@+id/info_phone"/>
	    </LinearLayout>
	    
	    <LinearLayout 
	         android:layout_width="match_parent"
	    	 android:layout_height="wrap_content"
	    	 android:orientation="horizontal">
	        
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="Website"/>
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="THE LINK"/>
	    </LinearLayout>
	</LinearLayout>
</LinearLayout>
